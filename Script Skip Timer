    };

    (function () {
        var H;
        for (var i in list) {
            H = list[i].host;
            switch (typeof H) {
            case 'string':
                H = new RegExp(H, 'i');
                break;
            case 'object':
                H = new RegExp(H.join('|'), 'i');
                break;
            }
            if (H.test(document.domain)) {
                fn = list[i].fn;
                addEvent(window, "DOMContentLoaded", fn);
                return;
            }
        }
    })();


})();
